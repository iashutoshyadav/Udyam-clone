generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  aadhaar   String   @unique
  otp       String?
  otpValid  Boolean  @default(false)
  pan       String?  @unique
  pin       String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
